  <modelVersion>4.0.0</modelVersion>
  <groupId>com.google.cloud</groupId>
  <artifactId>google-cloud-logging-parent</artifactId>
  <packaging>pom</packaging>
  <version>3.24.1-SNAPSHOT</version><!-- {x-version-update:google-cloud-logging:current} -->
  <name>Google Cloud Logging Parent</name>
  <url>https://github.com/googleapis/google-cloud-java</url>
  <description>
    Java idiomatic client for Google Cloud Platform services.
  </description>

  <parent>
    <groupId>com.google.cloud</groupId>
    <artifactId>google-cloud-jar-parent</artifactId>
    <version>1.78.0-SNAPSHOT</version><!-- {x-version-update:google-cloud-java:current} -->
    <relativePath>../google-cloud-jar-parent/pom.xml</relativePath>
